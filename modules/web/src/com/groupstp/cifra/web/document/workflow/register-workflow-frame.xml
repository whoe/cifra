<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        class="com.groupstp.cifra.web.document.workflow.RegisterWorkflowFrame"
        extends="/com/groupstp/cifra/web/document/workflow/document-workflow-frame.xml">
    <dsContext>
        <collectionDatasource id="documentDs"
                              class="com.groupstp.cifra.entity.Document"
                              view="document-wf-register-view"/>
    </dsContext>
    <layout>
        <table id="documentsTable">
            <buttonsPanel id="buttonsPanel">
                <button caption="msg://runButton"
                        icon="icons/ok.png"
                        invoke="run"/>
                <button caption="msg://cancelButton"
                        icon="icons/cancel.png"
                        invoke="problem"/>
            </buttonsPanel>
        </table>
    </layout>
</window>
