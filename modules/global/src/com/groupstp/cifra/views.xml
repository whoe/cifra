<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="com.groupstp.cifra.entity.DocType"
          extends="_local"
          name="docType-view">
        <property fetch="JOIN"
                  name="checkListItems"
                  view="_minimal"/>
    </view>
    <view class="com.groupstp.cifra.entity.Document"
          extends="_local"
          name="document-view">
        <property name="docType"
                  view="_minimal"/>
        <property name="file"
                  view="_minimal"/>
        <property name="checklist"
                  view="_minimal">
            <property name="item"
                      view="_minimal"/>
            <property name="checked"/>
            <property name="comment"/>
        </property>
        <property name="division"
                  view="_minimal"/>
        <property name="company"
                  view="_minimal"/>
        <property name="contragent"
                  view="_minimal"/>
        <property name="tag"
                  view="_minimal"/>
        <property name="description"/>
        <property name="number"/>
        <property name="date"/>
        <property name="warehouse"
                  view="_minimal"/>
        <property name="dateLoad"/>
        <property name="problems"/>
        <property name="fixDue"/>
        <property name="externalId"/>
        <property name="cell"/>
        <property name="externalLink"/>
    </view>
    <view class="com.groupstp.cifra.entity.CheckListItems"
          extends="_local"
          name="checkListItems-view">
        <property name="docType"
                  view="_minimal"/>
    </view>
    <view class="com.groupstp.cifra.entity.Division"
          extends="_local"
          name="division-view">
        <property name="company"
                  view="_minimal"/>
    </view>
    <view class="com.groupstp.cifra.entity.CheckList"
          extends="_local"
          name="checkList-view">
        <property name="document"
                  view="_minimal"/>
        <property name="item"
                  view="_minimal"/>
    </view>
    <view class="com.groupstp.cifra.entity.Contragent"
          extends="_local"
          name="contragent-view"/>
    <view class="com.groupstp.cifra.entity.Tag"
          extends="_minimal"
          name="tag-view">
        <property name="documents"
                  view="_minimal"/>
    </view>
    <view class="com.groupstp.cifra.entity.Employee"
          extends="_local"
          name="employee-view">
        <property name="user"
                  view="_minimal"/>
    </view>
    <view class="com.groupstp.cifra.entity.Document"
          extends="document-view"
          name="document-wf_view">
        <property name="initiator"
                  view="_minimal"/>
    </view>
    <view class="com.groupstp.cifra.entity.tasks.Task"
          extends="_local"
          name="tasks-full">
        <property name="author"
                  view="_minimal">
            <property name="user"
                      view="_minimal"/>
        </property>
        <property name="performer"
                  view="_minimal">
            <property name="user"
                      view="_minimal"/>
        </property>
        <property name="taskTypical"
                  view="_minimal">
            <property name="description"/>
            <property name="interval"/>
        </property>
        <property name="taskableEntity"
                  view="_minimal"/>
    </view>
    <view class="com.groupstp.cifra.entity.tasks.TaskTypical"
          extends="_local"
          name="taskTypical-full">
        <property name="taskTemplates"
                  view="_minimal">
            <property name="name"/>
            <property name="description"/>
        </property>
    </view>
    <view class="com.groupstp.cifra.entity.tasks.TaskTemplate"
          extends="_local"
          name="taskTemplate-full">
        <property name="tasks"
                  view="_minimal">
            <property name="description"/>
            <property name="interval"/>
            <property name="intervalType"/>
        </property>
    </view>
    <view class="com.haulmont.cuba.security.entity.User"
          extends="_minimal"
          name="user-with-role">
        <property name="userRoles"
                  view="_minimal">
            <property name="role"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.groupstp.workflowstp.entity.Workflow"
          extends="workflow-browse"
          name="workflow-browse">
        <property name="steps">
            <property name="stage"
                      view="_minimal">
                <property name="actorsRoles"
                          view="_minimal"/>
                <property name="browseScreenGroovyScript"/>
                <property name="actors"
                          view="_minimal"/>
            </property>
        </property>
    </view>
    <view class="com.groupstp.workflowstp.entity.Workflow"
          extends="workflow-browse"
          name="workflow-incoming-outgoing-register">
        <property name="active"/>
    </view>
    <view entity="cifra$Document"
          extends="document-wf_view"
          name="document-wf-register-view">
        <property name="workflow"/>
    </view>
</views>
